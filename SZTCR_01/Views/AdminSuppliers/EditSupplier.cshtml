@model SZTCR_01.Models.ViewModels.EditSupplierRequest
@{
}


<div>
    <h2 class="display-3">Edituj dobaljaca</h2>
</div>

<div class="container py-5">
    <form method="post">
        <div class="mb-3">
            <label class="form-label">ID</label>
            <input type="text" id="id" class="form-control" asp-for="Id" readonly/>
        </div>
        <div class="mb-3">
            <label class="form-label">Naziv za izmenu</label>
            <input type="text" id="nazivDobav" class="form-control" asp-for="NazivDobavljaca" />
        </div>
        <div class="mb-3">
            <label class="form-label">Adresa za izmenu</label>
            <input type="text" id="adresaDobav" class="form-control" asp-for="AdresaDobavljaca" />
        </div>
        <div class="mb-3">
            <label class="form-label">Email za izmenu</label>
            <input type="text" id="emailDobav" class="form-control" asp-for="EmailDobavljaca" />
        </div>

        <div class="mb-3">
            <button type="submit" class="btn btn-dark">Izmeni</button>
        </div>
    </form>
</div>